#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x91e760 .scope module, "test_andl" "test_andl" 2 3;
 .timescale -9 -9;
v0x963dd0_0 .var "a", 7 0;
v0x963ea0_0 .var "b", 7 0;
v0x963f50_0 .net "y", 7 0, v0x963d30_0; 1 drivers
S_0x953c90 .scope module, "a0" "andl" 2 7, 3 3, S_0x91e760;
 .timescale -9 -9;
v0x953d80_0 .net "a", 7 0, v0x963dd0_0; 1 drivers
v0x963c90_0 .net "b", 7 0, v0x963ea0_0; 1 drivers
v0x963d30_0 .var "y", 7 0;
E_0x91e690 .event edge, v0x963c90_0, v0x953d80_0;
    .scope S_0x953c90;
T_0 ;
    %wait E_0x91e690;
    %load/v 8, v0x953d80_0, 8;
    %load/v 16, v0x963c90_0, 8;
    %and 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x963d30_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x91e760;
T_1 ;
    %vpi_call 2 10 "$dumpfile", "andl.vcd";
    %vpi_call 2 11 "$dumpvars", 1'sb0, S_0x91e760;
    %end;
    .thread T_1;
    .scope S_0x91e760;
T_2 ;
    %vpi_call 2 15 "$monitor", $time, " ", "a=%b b=%b y=%b", v0x963dd0_0, v0x963ea0_0, v0x963f50_0;
    %end;
    .thread T_2;
    .scope S_0x91e760;
T_3 ;
    %movi 8, 240, 8;
    %set/v v0x963dd0_0, 8, 8;
    %set/v v0x963ea0_0, 0, 8;
    %delay 10, 0;
    %movi 8, 240, 8;
    %set/v v0x963dd0_0, 8, 8;
    %set/v v0x963ea0_0, 1, 8;
    %delay 10, 0;
    %movi 8, 240, 8;
    %set/v v0x963dd0_0, 8, 8;
    %movi 8, 170, 8;
    %set/v v0x963ea0_0, 8, 8;
    %delay 10, 0;
    %set/v v0x963dd0_0, 2, 8;
    %set/v v0x963ea0_0, 2, 8;
    %delay 10, 0;
    %vpi_call 2 28 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_andl.v";
    "andl.v";
