#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x123e730 .scope module, "test_lu" "test_lu" 2 3;
 .timescale -9 -9;
v0x1286e80_0 .var "a", 7 0;
v0x1286f50_0 .var "b", 7 0;
v0x1287000_0 .var "op", 1 0;
v0x12870b0_0 .net "y", 7 0, v0x1286dd0_0; 1 drivers
S_0x123e820 .scope module, "l0" "lu" 2 8, 3 8, S_0x123e730;
 .timescale -9 -9;
v0x1274b40_0 .net "a", 7 0, v0x1286e80_0; 1 drivers
v0x1286c90_0 .net "b", 7 0, v0x1286f50_0; 1 drivers
v0x1286d30_0 .net "op", 1 0, v0x1287000_0; 1 drivers
v0x1286dd0_0 .var "y", 7 0;
E_0x12506f0 .event edge, v0x1286d30_0, v0x1286c90_0, v0x1274b40_0;
    .scope S_0x123e820;
T_0 ;
    %wait E_0x12506f0;
    %load/v 8, v0x1286d30_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_0.3, 6;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1286dd0_0, 0, 2;
    %jmp T_0.5;
T_0.0 ;
    %load/v 8, v0x1274b40_0, 8;
    %load/v 16, v0x1286c90_0, 8;
    %and 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1286dd0_0, 0, 8;
    %jmp T_0.5;
T_0.1 ;
    %load/v 8, v0x1274b40_0, 8;
    %load/v 16, v0x1286c90_0, 8;
    %or 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1286dd0_0, 0, 8;
    %jmp T_0.5;
T_0.2 ;
    %load/v 8, v0x1274b40_0, 8;
    %load/v 16, v0x1286c90_0, 8;
    %xor 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1286dd0_0, 0, 8;
    %jmp T_0.5;
T_0.3 ;
    %load/v 8, v0x1274b40_0, 8;
    %load/v 16, v0x1286c90_0, 8;
    %nor 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1286dd0_0, 0, 8;
    %jmp T_0.5;
T_0.5 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x123e730;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "lu.vcd";
    %vpi_call 2 12 "$dumpvars", 1'sb0, S_0x123e730;
    %end;
    .thread T_1;
    .scope S_0x123e730;
T_2 ;
    %vpi_call 2 16 "$monitor", $time, " ", "a=%b b=%b op=%b y=%b", v0x1286e80_0, v0x1286f50_0, v0x1287000_0, v0x12870b0_0;
    %end;
    .thread T_2;
    .scope S_0x123e730;
T_3 ;
    %movi 8, 128, 8;
    %set/v v0x1286e80_0, 8, 8;
    %movi 8, 128, 8;
    %set/v v0x1286f50_0, 8, 8;
    %set/v v0x1287000_0, 0, 2;
    %delay 10, 0;
    %movi 8, 128, 8;
    %set/v v0x1286e80_0, 8, 8;
    %movi 8, 128, 8;
    %set/v v0x1286f50_0, 8, 8;
    %movi 8, 1, 2;
    %set/v v0x1287000_0, 8, 2;
    %delay 10, 0;
    %movi 8, 128, 8;
    %set/v v0x1286e80_0, 8, 8;
    %movi 8, 128, 8;
    %set/v v0x1286f50_0, 8, 8;
    %movi 8, 2, 2;
    %set/v v0x1287000_0, 8, 2;
    %delay 10, 0;
    %movi 8, 128, 8;
    %set/v v0x1286e80_0, 8, 8;
    %movi 8, 128, 8;
    %set/v v0x1286f50_0, 8, 8;
    %set/v v0x1287000_0, 1, 2;
    %delay 10, 0;
    %set/v v0x1286e80_0, 2, 8;
    %set/v v0x1286f50_0, 2, 8;
    %set/v v0x1287000_0, 2, 2;
    %delay 10, 0;
    %vpi_call 2 40 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_lu.v";
    "lu.v";
