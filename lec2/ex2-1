    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_0.3, 6;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1042800_0, 0, 2;
    %jmp T_0.5;
T_0.0 ;
    %load/v 8, v0xfff820_0, 8;
    %load/v 16, v0x10426c0_0, 8;
    %and 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1042800_0, 0, 8;
    %jmp T_0.5;
T_0.1 ;
    %load/v 8, v0xfff820_0, 8;
    %load/v 16, v0x10426c0_0, 8;
    %or 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1042800_0, 0, 8;
    %jmp T_0.5;
T_0.2 ;
    %load/v 8, v0xfff820_0, 8;
    %load/v 16, v0x10426c0_0, 8;
    %xor 8, 16, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1042800_0, 0, 8;
    %jmp T_0.5;
T_0.3 ;
    %load/v 8, v0xfff820_0, 8;
    %load/v 16, v0x10426c0_0, 8;
    %or 8, 16, 8;
    %inv 8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1042800_0, 0, 8;
    %jmp T_0.5;
T_0.5 ;
    %jmp T_0;
    .thread T_0, $push;
