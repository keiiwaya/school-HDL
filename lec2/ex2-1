v0xfff820_0 .net "a", 7 0, C4<zzzzzzzz>; 0 drivers
v0x10426c0_0 .net "b", 7 0, C4<zzzzzzzz>; 0 drivers
v0x1042760_0 .net "op", 1 0, C4<zz>; 0 drivers
v0x1042800_0 .var "y", 7 0;
E_0x10356a0 .event edge, v0x1042760_0, v0x10426c0_0, v0xfff820_0;
    .scope S_0xfff730;
T_0 ;
